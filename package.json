{
  "name": "repohunter",
  "version": "0.1.0",
  "description": "A service that lets you search for package dependencies in your organization's projects.",
  "private": false,
  "dependencies": {
    "material-ui": "^0.18.6",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "recompose": "^0.23.5",
    "rxjs": "^5.4.2"
  },
  "devDependencies": {
    "eslint": "^4.1.0",
    "eslint-plugin-prettier": "^2.1.2",
    "husky": "^0.14.1",
    "jest": "^20.0.4",
    "lint-staged": "^4.0.0",
    "prettier": "^1.5.3",
    "react-scripts": "1.0.10"
  },
  "scripts": {
